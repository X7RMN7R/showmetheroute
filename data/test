MONGO INSERT
db.track.insert({ "_id" : "First track", "_class" : "com.showmetheroute.persistence.domain.Track", "wayPoints" : [ { "_id" : "Nantes", "latitude" : 47.21806, "longitude" : -1.55278 }, { "_id" : "Special waypoint", "latitude" : 48.390834, "longitude" : -4.485556 } ] })

CURL POST
curl -i -H "Content-Type: application/json" -d {\"_id\":\"First track\",\"wayPoints\":[{\"_id\":\"Nantes\",\"latitude\":47.21806,\"longitude\":-1.55278},{\"_id\":\"Special waypoint\",\"latitude\":48.390834,\"longitude\":-4.485556}]} http://localhost:8080/tracks